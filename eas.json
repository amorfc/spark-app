{
	"cli": {
		"version": ">= 16.9.0",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"NODE_ENV": "development"
			}
		},
		"production": {
			"distribution": "store",
			"developmentClient": false,
			"env": {
				"NODE_ENV": "production"
			},
			"ios": {
				"simulator": false
			},
			"android": {
				"gradleCommand": ":app:bundleRelease"
			},
			"autoIncrement": true
		}
	},
	"submit": {
		"production": {
			"ios": {
				"appleId": "SparkCompanyTR@gmail.com",
				"ascAppId": "6747034934"
			},
			"android": {
				"track": "production"
			}
		},
		"internal": {
			"android": {
				"track": "internal"
			}
		}
	}
}
